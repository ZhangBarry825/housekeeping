.page {
  width: 100%;
}
.page .content-box {
  width: 100%;
  background-color: #f4f4f4;
  padding: 10rpx 10rpx;
  box-sizing: border-box;
}
.page .content-box .clean {
  padding: 10rpx 0;
  width: 100%;
}
.page .content-box .clean .title {
  display: flex;
  justify-content: space-between;
  flex-direction: row;
  padding: 10rpx 0;
  align-items: center;
}
.page .content-box .clean .title .left {
  font-weight: bold;
  font-size: 28rpx;
  color: #2c2c2c;
}
.page .content-box .clean .title .right {
  width: 30rpx;
  height: 30rpx;
}
.page .content-box .weui-cell__bd {
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #5a5a5a;
}
