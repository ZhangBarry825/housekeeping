.page {
  padding: 10rpx 0;
}
.page .item1 {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .item1 .item {
  display: flex;
  flex-direction: row;
  padding: 15rpx;
  box-sizing: border-box;
  background-color: #fff;
  align-items: center;
  margin-top: 5rpx;
}
.page .item1 .item .addressInput {
  width: 480rpx;
}
.page .item1 .item .left {
  width: 160rpx;
  padding: 0 20rpx;
  border-right: 1px solid gainsboro;
  font-size: 28rpx;
}
.page .item1 .item .left image {
  width: 30rpx;
  height: 30rpx;
}
.page .item1 .item .left1 {
  height: 200rpx;
  display: flex;
  align-items: center;
}
.page .item1 .item .left1 image {
  width: 50rpx;
  height: 50rpx;
}
.page .item1 .item .right {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  font-size: 28rpx;
}
.page .item1 .item .right .avatar {
  width: 200rpx;
  height: 200rpx;
  border: 1px solid gainsboro;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #999;
}
.page .item1 .item .right .avatar image {
  width: 60rpx;
  height: 60rpx;
}
.page .item1 .item .right .avatar .image1 {
  width: 100%;
  height: 100%;
}
.page .item2 {
  margin-top: 10rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.page .item2 .title {
  display: flex;
  flex-direction: row;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  align-items: center;
}
.page .item2 .title .text1 {
  font-weight: bold;
}
.page .item2 .title .text2 {
  margin-left: 20rpx;
  font-size: 25rpx;
  color: #5a5a5a;
}
.page .item2 .item {
  display: flex;
  flex-direction: row;
  padding: 15rpx;
  box-sizing: border-box;
  background-color: #fff;
  align-items: center;
  margin-top: 5rpx;
}
.page .item2 .item .left {
  width: 150rpx;
  padding: 0 20rpx;
  border-right: 1px solid gainsboro;
  font-size: 28rpx;
}
.page .item2 .item .left image {
  width: 30rpx;
  height: 30rpx;
}
.page .item2 .item .right {
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  font-size: 28rpx;
}
.page .item2 .pictures {
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 15rpx;
  box-sizing: border-box;
  background-color: #fff;
  align-items: center;
  margin-top: 5rpx;
}
.page .item2 .pictures .line1,
.page .item2 .pictures .line2,
.page .item2 .pictures .line3 {
  padding: 10rpx 0;
  font-size: 25rpx;
}
.page .item2 .pictures .line1 {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.page .item2 .pictures .line1 .pic {
  width: 300rpx;
  height: 200rpx;
  border: 1px solid gainsboro;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #999;
}
.page .item2 .pictures .line1 .pic image {
  width: 60rpx;
  height: 60rpx;
}
.page .item2 .pictures .line1 .pic .image1 {
  width: 100%;
  height: 100%;
}
.page .item2 .pictures .line2 {
  width: 100%;
  color: #5a5a5a;
  padding: 20rpx 20rpx;
}
.page .item2 .pictures .line3 {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.page .item2 .pictures .line3 .pic {
  width: 300rpx;
  height: 200rpx;
  border: 1px solid gainsboro;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #999;
}
.page .item2 .pictures .line3 .pic image {
  width: 60rpx;
  height: 60rpx;
}
.page .item2 .pictures .line3 .pic .image1 {
  width: 100%;
  height: 100%;
}
.page .item2 .pictures .line3 .tips {
  width: 300rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.page .item2 .pictures .line3 .tips text {
  color: #5a5a5a;
}
.page .item2 .pictures .line3 .tips .example {
  color: #00c87f;
}
.page .item3 {
  margin-top: 10rpx;
  background-color: #fff;
}
.page .item3 .weui-agree__link {
  color: #00c87e;
}
.page .item3 .button-box {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 50rpx 0;
}
.page .item3 .button-box .button {
  padding: 15rpx 0;
  width: 70%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 30rpx;
  color: #fff;
  background-color: #02c87d;
}
