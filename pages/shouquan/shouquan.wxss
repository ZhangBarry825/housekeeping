  margin-left: 21rpx;
  margin-top: 24rpx;
  position: relative;
}
.sous_right button{
  position: absolute;
  top:0px;
  left:0px;
  width:100%;
  height: 100%;
  opacity: 0;
}
.sous_right  image{
  display:block;
  width: 100%;
  height: 100%;
}
.nav{
  width: 100%;
  height:70rpx;
  background:#fff;
  position:fixed;
  top:91rpx;left: 0;
  z-index:99;
}
.nav_nei{
  width: 100%;
  height:70rpx;
  white-space: nowrap;
}
.nav_nei view{
  display:inline-block;
  margin-left:50rpx;
  height:65rpx;
