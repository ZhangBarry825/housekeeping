  font-size:28rpx;
  color:#000000;
  line-height:65rpx;
  border-bottom:5rpx solid #fff;
}
.nav_nei view:nth-child(1){
  margin-left:25rpx;
}
.nav_nei view:last-child{
  margin-right:25rpx;
}
